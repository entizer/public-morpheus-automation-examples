{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "keyname": {
            "Type": "String"
        },
        "imageid": {
            "Type": "String",
            "Default": "ami-024e6efaf93d85776"
        },
        "subnetid": {
            "Type": "String"
        }
    },
    "Resources": {
        "EC2Instance": {
            "Type": "AWS::EC2::Instance",
            "Properties": {
                "ImageId": { "Ref": "imageid" },
                "InstanceType": "t2.micro",
                "KeyName": { "Ref": "keyname" },
                "SecurityGroupIds": [
                    {"Fn::ImportValue" : "SecurityGroup-ID"}
                ],
                "SubnetId": { "Ref": "subnetid" }
            }
        }
    }
}